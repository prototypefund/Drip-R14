{
  "name": "drip",
  "version": "0.0.1",
  "description": "A menstrual cycle tracking app that's open-source and leaves your data on your phone. Use it to track your menstrual cycle or for fertility awareness!",
  "keywords": [
    "cycle-tracking",
    "symptothermal"
  ],
  "homepage": "https://gitlab.com/bloodyhealth/drip#README",
  "bugs": {
    "url": "https://gitlab.com/bloodyhealth/drip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/bloodyhealth/drip.git"
  },
  "license": "GPL-3.0-or-later",
  "author": "",
  "contributors": [
    "Julia Friesel <julia.friesel@gmail.com>",
    "Marie Kochsiek",
    "Tina Baumann"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "android": "react-native run-android",
    "devtool": "adb shell input keyevent 82",
    "format": "prettier --write '**/*.js'; eslint --fix '**/*.js'; import-sort **/*.js --write; sort-package-json",
    "lint": "eslint components lib test",
    "log": "react-native log-android",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "mocha --recursive --require @babel/register test && npm run lint",
    "test-watch": "mocha --recursive --require @babel/register --watch test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "import-sort --write",
      "git add"
    ],
    "*.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "ajv": "^5.5.2",
    "assert": "^1.4.1",
    "csvtojson": "^2.0.8",
    "date-range": "0.0.2",
    "isobject": "^3.0.1",
    "js-base64": "^2.4.8",
    "js-joda": "^1.8.2",
    "moment": "^2.22.2",
    "nodejs-mobile-react-native": "^0.3.0",
    "object-path": "^0.11.4",
    "obv": "0.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-native": "^0.57.8",
    "react-native-calendars": "^1.19.3",
    "react-native-document-picker": "^2.1.0",
    "react-native-fs": "^2.13.3",
    "react-native-hyperlink": "0.0.14",
    "react-native-modal-datetime-picker-nevo": "^4.11.0",
    "react-native-push-notification": "^3.1.1",
    "react-native-restart": "0.0.7",
    "react-native-share": "^1.1.3",
    "react-native-vector-icons": "^5.0.0",
    "realm": "^2.22.0",
    "sympto": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^1.3.1",
    "import-sort-cli": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^8.1.4",
    "metro-react-native-babel-preset": "^0.51.1",
    "mocha": "^5.2.0",
    "prettier": "1.16.4",
    "sort-package-json": "^1.19.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "rnpm": {
    "assets": [
      "assets/fonts"
    ]
  }
}
